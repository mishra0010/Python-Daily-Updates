from array import *

a1 = array('i',[23,43,45])
# print(type(a1))
# print((a1))
for x in a1:
    print(x)
    
for i in range(3):
    print(a1[i],end=' ')
    
print("\n")

i = 0
while(i<len(a1)):
    print(a1[i])
    i+=1
    
a1.append(20)
print(a1)
print(a1.count(20))
print(a1.index(43))

# In pop function we provide index
a1.pop()
print(a1)
a1.pop(0)
print(a1)

# In remove functon we tell the value 
a1.append(43)
a1.remove(43)
print(a1)
